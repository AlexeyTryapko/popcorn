    "passport-local": "^1.0.0",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.13",
    "sentiment": "^5.0.1",
    "socket.io": "^2.2.0",
    "ts-node": "^8.3.0",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/multiparty": "^0.0.32",
    "@types/passport-local": "^1.0.33",
    "husky": "^3.0.3",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  }
