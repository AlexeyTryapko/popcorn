    "watch": [
      "./"
    ],
    "exec": "./node_modules/.bin/ts-node ./index.ts",
    "ext": "ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.9",
    "@types/passport-jwt": "^3.0.1",
    "aws-sdk": "^2.521.0",
    "base64-img": "^1.0.4",
    "bluebird": "^3.5.5",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.4.0",
    "fs": "^0.0.1-security",
    "image-to-base64": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multiparty": "^4.2.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
