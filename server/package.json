{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "start-server": "ts-node ./",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "./git",
      "node_modules"
